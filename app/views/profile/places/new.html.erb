
<div class="text-center">
  <h1> Add a new place to rent  </h1>
  <div class=" form-inline">
  <%= form_for([:profile, @place]) do |f| %>
  <%#= form_for(@place, url: profile_places_path) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :kind %>
    <%= f.text_field :kind %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :day_price %>
    <%= f.text_field :day_price %>

    <%= f.label :night_price %>
    <%= f.text_field :night_price %>

    <%= f.submit %>
    <% end %>
  </div>
</div>
