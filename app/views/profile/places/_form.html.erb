<ul>
  <%= simple_form_for([:profile, @place]) do |f| %>
  <%#= form_for(@place, url: profile_places_path) do |f| %>
  <%= f.input :name %>

  <%= f.input :kind %>

  <%= f.input :address %>

  <%= f.input :zip_code %>

  <%= f.input :city %>

  <%= f.input :country, priority: [ 'BE' ] %>

  <%= f.input :description %>

  <%= f.input :day_price %>

  <%= f.input :photo %>

  <%= f.input :photo_cache, as: :hidden %>

  <%= f.submit class:"btn btn-treehouse" %>
  <% end %>
</ul>

